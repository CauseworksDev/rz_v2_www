<html lang="ko">
  <% include ../inc/head_script.ejs %>
  <link rel="stylesheet" href="/public/test/css/program.css" />
  <script src="/views/user/program/shop.js" type="text/javascript"></script>
  <script>
    $(document).ready(function () {
      let oldVal = '';
      $('#searchKeyword').on('propertychange change keyup paste input', function () {
        let currentVal = $(this).val();
        if (currentVal == oldVal) {
          return;
        }
        oldVal = currentVal;
        getShopInfo();
      });
      getShopInfo();
    });
  </script>
  <body>
    <div id="wrap">
      <div id="container">
        <section class="shop_sec_01">
          <div class="inner">
            <div class="logo">
              <img src="/public/test/images/program/shop_sec_01_img_01.png" alt="가플지우 지켜와요 플라스틱 지겨카요 우리바다" />
            </div>
            <h2 class="title">이마트 점포매장</h2>
            <div class="form_wrap">
              <label class="hidden" for=""></label>
              <input type="text" id="searchKeyword" placeholder="매장명 검색" />
              <button type="submit" onclick="getShopInfo()"></button>
            </div>
            <div class="list_wrap">
              <ul id="tbl_dataList"></ul>
            </div>
          </div>
          <!-- //inner -->
        </section>
      </div>
      <!-- //container -->
    </div>
    <!-- //wrap -->
  </body>
</html>
